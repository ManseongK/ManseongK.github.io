<script>
    console.log('Before form HTML');
    console.log('document.frm1', document.frm1);
    console.log('document.forms.frm1', document.forms.frm1);


    console.log('document.frm2', document.frm2);
    console.log('document.forms.frm2', document.forms.frm2);
    console.log('---------');
</script>
<form id="frm1">
    <input type="submit" />
</form>
<form name="frm2" id="frm2">
    <input type="submit" />
</form>
<script>
    console.log('After form HTML');
    console.log('document.frm1', document.frm1);
    console.log('document.forms.frm1', document.forms.frm1);


    console.log('document.frm2', document.frm2);
    console.log('document.forms.frm2', document.forms.frm2);
    console.log('---------');
</script>
